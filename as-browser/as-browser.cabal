library
    exposed-modules:
        Language.Haskell.AsBrowser
    build-depends:
        base >=4,
        directory -any,
        filepath -any,
        safecopy -any,
        acid-state -any,
        ixset-typed -any,
        text -any,
        Cabal -any,
        time -any,
        data-default -any,
        mtl -any,
        conduit -any,
        conduit-extra -any,
        http-conduit -any,
        bytestring -any,
        zlib -any,
        tar -any,
        utf8-string -any,
        parallel-io -any,
        containers -any,
        aeson -any,
        network-uri -any,
        xml-conduit -any,
        html-conduit -any,
        blaze-html -any,
        transformers -any
    hs-source-dirs: src
    other-modules:
        Language.Haskell.ASBrowser.Operations.Decls
        Language.Haskell.ASBrowser.Types
        Language.Haskell.ASBrowser.Operations.Packages
        Language.Haskell.ASBrowser.Utils
        Language.Haskell.ASBrowser.Database
        Language.Haskell.ASBrowser.Operations.Modules
        Language.Haskell.ASBrowser.Integration.Cabal
        Language.Haskell.ASBrowser.Integration.Files
        Language.Haskell.ASBrowser.Operations.Components
        Language.Haskell.ASBrowser.Integration.Src
    ghc-options: -Wall -O2

executable as-browser
    main-is: Main.hs
    build-depends:
        base >=4,
        as-browser -any,
        directory -any,
        filepath -any,
        safecopy -any,
        acid-state -any,
        ixset-typed -any,
        text -any,
        Cabal -any,
        time -any,
        data-default -any,
        mtl -any,
        conduit -any,
        conduit-extra -any,
        http-conduit -any,
        bytestring -any,
        zlib -any,
        tar -any,
        utf8-string -any,
        parallel-io -any,
        containers -any,
        aeson -any,
        snap-core -any,
        snap-server -any,
        snaplet-acid-state -any,
        snap -any,
        lens -any,
        transformers -any,
        network-uri -any,
        xml-conduit -any,
        html-conduit -any,
        blaze-html -any,
        attoparsec -any
    hs-source-dirs: exe
    ghc-options: -Wall -rtsopts -threaded -O2

test-suite as-browser-test
    type: exitcode-stdio-1.0
    main-is: as-browser-test.hs
    build-depends:
        base >=4,
        tasty >0.10,
        tasty-hunit -any,
        tasty-quickcheck -any,
        QuickCheck -any,
        quickcheck-instances -any,
        HUnit -any,
        Cabal -any,
        acid-state -any,
        ixset-typed -any,
        safecopy -any,
        text -any,
        time -any,
        data-default -any,
        directory -any,
        filepath -any,
        conduit -any,
        conduit-extra -any,
        http-conduit -any,
        bytestring -any,
        mtl -any,
        zlib -any,
        tar -any,
